{
    "responseOverrides": {
        "404": {
            "rewrite": "/docs/404.html"
        }
    },
    "routes": [
        {
            "route": "/*.{jpg,gif,png,ico,svg,css,scss,js,webp,webmanifest}",
            "headers": {
                "cache-control": "public, max-age=604800, immutable"
            }
        }
    ]
}
