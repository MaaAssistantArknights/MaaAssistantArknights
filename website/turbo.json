{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".vuepress/dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:home": {
      "cache": false
    },
    "dev:docs": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:js": {
      "cache": false
    },
    "lint:prettier": {
      "cache": false
    },
    "lintfix": {
      "cache": false
    }
  }
}
