{
    "MiniGame@SecretFront@Begin": {
        "doc": "隐秘战线（入口名交由插件判定结局，默认入口：结局E）",
        "algorithm": "JustReturn",
        "next": ["MiniGame@SecretFront@Run"]
    },
    "MiniGame@SecretFront@Run": {
        "doc": "从隐秘战线主界面开始，后续交由插件控制路线与结局",
        "algorithm": "JustReturn",
        "next": ["MiniGame@SecretFront@End", "MiniGame@SecretFront@SelectTeam"]
    },
    "MiniGame@SecretFront@Actions": {
        "doc": "隐秘战线：行动列表（插件根据入口名决定结局与路线）",
        "algorithm": "JustReturn",
        "next": ["MiniGame@SecretFront@AfterChooseWait"]
    },
    "MiniGame@SecretFront@AfterChooseWait": {
        "algorithm": "JustReturn",
        "next": ["MiniGame@SecretFront@ContinueResult", "MiniGame@SecretFront@End", "MiniGame@SecretFront@EventTeam"]
    },
    "MiniGame@SecretFront@TapTeam@Management": {
        "doc": "选择小队：物资分队",
        "template": "MiniGame@SecretFront@TeamManagement.png",
        "roi": [0, 150, 250, 500],
        "action": "ClickSelf",
        "next": ["MiniGame@SecretFront@SelectTeamOK"]
    },
    "MiniGame@SecretFront@TapTeam@Intelligence": {
        "doc": "选择小队：情报分队",
        "template": "MiniGame@SecretFront@TeamIntelligence.png",
        "baseTask": "MiniGame@SecretFront@TapTeam@Management"
    },
    "MiniGame@SecretFront@TapTeam@Medicine": {
        "doc": "选择小队：医疗分队",
        "template": "MiniGame@SecretFront@TeamMedicine.png",
        "baseTask": "MiniGame@SecretFront@TapTeam@Management"
    },
    "MiniGame@SecretFront@SelectTeam": {
        "doc": "选择小队界面锚点",
        "template": "MiniGame@SecretFront@SelectTeam.png",
        "roi": [850, 160, 250, 150],
        "preDelay": 2000,
        "next": ["MiniGame@SecretFront@ActionContinue"]
    },
    "MiniGame@SecretFront@SelectTeamOK": {
        "doc": "选择分队：右下角确认出发",
        "template": "MiniGame@SecretFront@SelectTeamOK.png",
        "roi": [850, 580, 400, 100],
        "action": "ClickSelf"
    },
    "MiniGame@SecretFront@ActionContinue": {
        "doc": "继续前进",
        "roi": [150, 500, 250, 150],
        "action": "ClickSelf",
        "postDelay": 500,
        "next": ["MiniGame@SecretFront@RouteSelect", "#self"]
    },
    "MiniGame@SecretFront@RouteSelect": {
        "roi": [0, 280, 150, 230],
        "preDelay": 1500,
        "next": [
            "MiniGame@SecretFront@EventDetected",
            "MiniGame@SecretFront@PreClickDetect",
            "MiniGame@SecretFront@ClickThenActionsDetected"
        ]
    },
    "MiniGame@SecretFront@PreClickDetect": {
        "doc": "隐秘战线：在默认点击前做一次事件识别（供插件拦截并在当前页选卡）",
        "algorithm": "OcrDetect",
        "text": ["支援作战平台", "游侠", "诡影迷踪"],
        "roi": [200, 320, 1020, 200],
        "next": ["MiniGame@SecretFront@ActionsDetected", "MiniGame@SecretFront@RouteSelect"]
    },
    "MiniGame@SecretFront@ClickThenActionsDetected": {
        "template": "MiniGame@SecretFront@RouteSelect.png",
        "roi": [0, 280, 150, 230],
        "action": "ClickRect",
        "specificRect": [363, 426, 1, 1],
        "postDelay": 1500,
        "next": ["MiniGame@SecretFront@ActionsDetected", "MiniGame@SecretFront@RouteSelect"]
    },
    "MiniGame@SecretFront@ContinueResult": {
        "doc": "行动结果：继续",
        "roi": [950, 590, 280, 70],
        "action": "ClickSelf",
        "postDelay": 500,
        "next": ["MiniGame@SecretFront@ActionContinue", "MiniGame@SecretFront@EventContinue", "#self"]
    },
    "MiniGame@SecretFront@EventContinue": {
        "doc": "应对危机事件事件",
        "roi": [150, 500, 250, 150],
        "action": "ClickSelf",
        "postDelay": 500,
        "next": ["MiniGame@SecretFront@RouteSelect", "MiniGame@SecretFront@ClickAnywhere", "#self"]
    },
    "MiniGame@SecretFront@ClickAnywhere": {
        "doc": "任意位置继续，下一层或者重开",
        "template": "MiniGame@SecretFront@ClickAnywhere.png",
        "roi": [300, 550, 600, 150],
        "action": "ClickSelf",
        "next": ["MiniGame@SecretFront@ActionContinue", "MiniGame@SecretFront@SelectTeam", "#self"]
    },
    "MiniGame@SecretFront@End": {
        "doc": "结局完成：点任意位置推进到退出界面",
        "roi": [700, 550, 200, 150],
        "maskRange": [90, 255],
        "action": "ClickSelf",
        "next": ["#self", "Stop"]
    },
    "MiniGame@SecretFront@ActionsDetected": {
        "doc": "行动列表识别锚点（正常带数值的行动列表，命中后进入 Actions 钩子节点触发插件）",
        "template": "MiniGame@SecretFront@Card.png",
        "roi": [180, 300, 1050, 180],
        "maskRange": [1, 255],
        "next": ["MiniGame@SecretFront@Actions"]
    },
    "MiniGame@SecretFront@EventDetected": {
        "doc": "行动列表识别锚点（突发事件的可选择卡牌）",
        "template": "MiniGame@SecretFront@Available.png",
        "roi": [190, 580, 850, 60],
        "maskRange": [1, 255],
        "next": ["MiniGame@SecretFront@Actions"]
    },
    "MiniGame@SecretFront@EventTeam": {
        "action": "ClickSelf",
        "roi": [250, 200, 500, 250],
        "next": ["MiniGame@SecretFront@ClickAnywhere", "#self"]
    },
    "MiniGame@SecretFront@Number": {
        "doc": "隐秘战线：数字 OCR 配置（供插件调用）",
        "baseTask": "NumberOcrReplace",
        "ocrReplace": [["[^\\d]", ""]],
        "binThreshold": [120, 255],
        "useRaw": false
    },
    "MiniGame@SecretFront@ReadProperty-Materiel": {
        "baseTask": "MiniGame@SecretFront@Number",
        "roi": [70, 327, 50, 23]
    },
    "MiniGame@SecretFront@ReadProperty-Intelligence": {
        "baseTask": "MiniGame@SecretFront@Number",
        "roi": [70, 387, 50, 23]
    },
    "MiniGame@SecretFront@ReadProperty-Medicine": {
        "baseTask": "MiniGame@SecretFront@Number",
        "roi": [70, 450, 50, 23]
    },
    "MiniGame@SecretFront@ReadCard-Materiel": {
        "baseTask": "MiniGame@SecretFront@Number",
        "rectMove": [58, 47, 64, 20]
    },
    "MiniGame@SecretFront@ReadCard-Intelligence": {
        "baseTask": "MiniGame@SecretFront@Number",
        "rectMove": [168, 47, 64, 20]
    },
    "MiniGame@SecretFront@ReadCard-Medicine": {
        "baseTask": "MiniGame@SecretFront@Number",
        "rectMove": [276, 47, 64, 20]
    },
    "MiniGame@SecretFront@ReadCard-Percentage": {
        "baseTask": "MiniGame@SecretFront@Number",
        "rectMove": [7, 293, 80, 22]
    },
    "MiniGame@SecretFront@EventReplace": {
        "algorithm": "OcrDetect",
        "text": [],
        "doc": "隐秘战线：事件 OCR 替换配置，结局统一采用 4X 形式，roi.x y for rectmove",
        "ocrReplace": [
            [" ", ""],
            ["结局", "4"]
        ],
        "roi": [7, 253, 86, 35]
    },
    "MiniGame@SecretFront@ClickNextPage": {
        "algorithm": "JustReturn",
        "action": "ClickRect",
        "specificRect": [1190, 150, 1, 1]
    },
    "MiniGame@SecretFront@ClickPrevPage": {
        "algorithm": "JustReturn",
        "action": "ClickRect",
        "specificRect": [233, 150, 1, 1]
    }
}
